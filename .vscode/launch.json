    "version": "0.2.0",
    "configurations": [
        {
            "name": "üîç Debug Rastreo de datos desde Fuentes deportivas para extracci√≥n de datos - Sports_ingestion",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/backend/main_init_scripts.py",
            "console": "integratedTerminal",
            "args": [
                "--process",
                "proceso_rastreo_data_fuentes_deportivas"
            ]
        },
        {
            "name": "üîç Debug TipsterByte FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main_init_web_server:app",
                "--host", "127.0.0.1",
                "--port", "8010",
                "--reload"
            ],
            "cwd": "${workspaceFolder}/backend",
            "console": "integratedTerminal",
            "jinja": true
        },        
    ]
}

